<%= link_to 'top 100', urls_path %>
<%= form_for @url do |f| %>
  <% if @url.errors.any? %>
    <% @url.errors.messages.each do |attr, msg| %>
      <%= msg.join('and ') %><br/>
    <% end %>
  <% end %>
  <%= f.label :original, 'Url' %>
  <%= f.text_field :original %>
  <br/>
  <%= f.submit %>
<% end %>